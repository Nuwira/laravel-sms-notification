{
    "name": "nuwira/laravel-sms-notification",
    "description": "Nuwira SMS Notifications for Laravel",
    "homepage": "https://github.com/Nuwira/laravel-sms-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Zamroni",
            "email": "zam@nuwira.co.id",
            "homepage": "https://matriphe.com",
